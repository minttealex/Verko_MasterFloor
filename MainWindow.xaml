<Window x:Class="Verko_MasterFloor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Verko_MasterFloor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontFamily="Segoe UI" Icon="ress/Мастер пол.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        
        <Image Source="ress/Мастер пол.png" HorizontalAlignment="Left"></Image>

        <TextBlock Text="Мастер Пол" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>

        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>

        <Button Content="Back" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"></Button>
    </Grid>
</Window>
